    "graphql-yoga": "^5.18.0",
    "md-editor-v3": "^6.3.1",
    "nitro": "https://pkg.pr.new/nitrojs/nitro@99691fc",
    "nitro-graphql": "^2.0.0-beta.65",
    "notivue": "^2.4.5",
    "pg": "^8.16.3",
    "pinia": "^3.0.4",
    "pinia-plugin-persistedstate": "^4.7.1",
    "reka-ui": "^2.7.0",
    "tailwind-merge": "^3.4.0",
    "tailwindcss": "^4.1.18",
    "tw-animate-css": "^1.4.0",
    "unstorage": "^2.0.0-alpha.5",
    "uuid": "^13.0.0",
    "vaul-vue": "^0.4.1",
    "vee-validate": "5.0.0-beta.0",
    "vite-tsconfig-paths": "^6.0.4",
    "vue-input-otp": "^0.3.2",
    "vue-router": "^4.6.4",
    "vue-sonner": "^2.0.9",
    "vue-tsc": "^3.2.2",
    "zod": "^4.3.5"
  },
  "devDependencies": {
    "@antfu/eslint-config": "^7.0.0",
    "@nuxt/eslint": "latest",
    "@nuxt/kit": "https://pkg.pr.new/@nuxt/kit@33005",
    "@nuxt/module-builder": "^1.0.2",
    "@nuxt/schema": "https://pkg.pr.new/@nuxt/schema@33005",
    "@types/node": "^25.0.8",
    "@types/pg": "^8.16.0",
    "eslint": "^9.39.2",
    "nuxt": "https://pkg.pr.new/nuxt@33005",
    "shadcn-vue": "^2.4.3",
    "typescript": "^5.9.3",
    "vue": "^3.5.26"
  },
  "resolutions": {
    "abckit": "workspace:*"
  },
  "overrides": {
    "@nuxt/kit": "https://pkg.pr.new/@nuxt/kit@33005",
    "@nuxt/schema": "https://pkg.pr.new/@nuxt/schema@33005",
    "entities": "7.0.0",
    "nuxt": "https://pkg.pr.new/nuxt@33005"
  },
  "patchedDependencies": {
    "entities@7.0.0": "patches/entities@7.0.0.patch",
    "entities@7.0.0@7.0.0": "patches/entities@7.0.0.patch"
  }
}
