    "@sentry/nuxt": "^10.33.0",
    "@sindresorhus/slugify": "^3.0.0",
    "@tailwindcss/typography": "^0.5.19",
    "@tanstack/vue-table": "^8.21.3",
    "@unovis/vue": "^1.6.2",
    "@vee-validate/nuxt": "5.0.0-beta.0",
    "@vueuse/core": "^14.1.0",
    "@vueuse/nuxt": "^14.1.0",
    "@vueuse/router": "^14.1.0",
    "@vueuse/sound": "^2.1.3",
    "apiful": "^4.0.1",
    "aws4fetch": "^1.0.20",
    "better-auth": "^1.4.12",
    "capacitor-native-settings": "^7.0.2",
    "class-variance-authority": "^0.7.1",
    "clsx": "^2.1.1",
    "consola": "^3.4.2",
