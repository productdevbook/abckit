    "@capacitor/status-bar": "^8.0.0",
    "@capgo/capacitor-social-login": "^8.2.13",
    "@graphql-tools/utils": "^11.0.0",
    "@ionic/vue": "^8.7.16",
    "@nuxt/icon": "^2.2.0",
    "@nuxt/scripts": "^0.13.2",
    "@nuxtjs/color-mode": "^4.0.0",
    "@nuxtjs/i18n": "^10.2.1",
    "@nuxtjs/ionic": "^1.0.2",
    "@nuxtjs/tailwindcss": "7.0.0-beta.1",
    "@openfga/sdk": "^0.9.1",
    "@pinia/colada": "^0.21.1",
    "@pinia/colada-nuxt": "^0.3.0",
